# Turso Database - Required for audit storage
TURSO_DATABASE_URL=libsql://your-db-name.turso.io
TURSO_AUTH_TOKEN=your_auth_token

# SEMrush API - Required for SEO metrics
SEMRUSH_API_KEY=your_semrush_api_key

# DataForSEO - Required for additional SEO data
DATAFORSEO_LOGIN=your_login
DATAFORSEO_PASSWORD=your_password

# Google PageSpeed Insights - Optional (works without, but rate limited)
PAGESPEED_API_KEY=your_google_api_key

# Google Analytics 4 - Optional (for report tracking)
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX
# Optional: Google Ads conversion tracking for lead capture
# NEXT_PUBLIC_GA_CONVERSION_LEAD_CAPTURE=AW-XXXXXXXXX/XXXXX
